{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Android",
            "request": "launch",
            "type": "reactnative",
            "cwd": "${workspaceFolder}",
            "enableDebug": false,
            "platform": "android"
        },
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "enter program name, for example ${workspaceFolder}/a.out",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "type": "OctaveDebugger",
            "request": "launch",
            "name": "Execute selected file.",
            "program": "${file}",
            "octave": "octave-cli",
            "sourceFolder": "${workspaceFolder}"
        },
        {
            "type": "OctaveDebugger",
            "request": "launch",
            "name": "Execute selected file.",
            "program": "${file}",
            "octave": "octave-cli",
            "sourceFolder": "${workspaceFolder}"
        },
        {
            "type": "OctaveDebugger",
            "request": "launch",
            "name": "Execute selected file.",
            "program": "${file}",
            "octave": "octave-cli",
            "sourceFolder": "${workspaceFolder}"
        },
        {
            "type": "octave",
            "request": "launch",
            "name": "Octave Debug Adapter(preview)",
            "exec": "octave-cli",
            "program": "${command:AskForProgramName}"
        },
        {
            "name": "C/C++: gcc-11 build and debug active file",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}/${fileBasenameNoExtension}",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "C/C++: gcc-11 build active file",
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}